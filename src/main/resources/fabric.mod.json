{
  "schemaVersion": 1,
  "id": "unnamed-meteor-addon",
  "version": "${version}",
  "name": "Unnamed Addon",
  "description": "Addons, yay.",
  "authors": [
    "blindedbythesun"
  ],
  "contact": {
    "repo": "https://github.com/blindedbythesun/unnamed-meteor-addons"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "me.sun.unnamed.Addon"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
